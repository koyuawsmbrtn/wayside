{
  "layer": "top",
  "position": "right",
  "exclusive": true,
  "modules-right": ["custom/todo", "custom/gcal", "custom/debug"],
  "width": 200,
  "cursor-width": 0,
  "custom/todo": {
    "exec": "python ~/.config/wayside/convert_text.py ~/todo.txt",
    "interval": 3
  },
  "custom/gcal": {
    "exec": "cd ~/.config/wayside && python ~/.config/wayside/gcal_fetch.py --quiet",
    "interval": 300,
    "allow-breaks": true
  }
}
