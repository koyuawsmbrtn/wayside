{
  "layer": "top",
  "position": "right",
  "exclusive": true,
  "modules-right": ["custom/todo", "custom/gcal", "custom/debug"],
  "width": 200,
  "custom/todo": {
    "exec": "python ~/.config/wayside/convert_text.py ~/todo.txt",
    "interval": 3
  },
  "custom/gcal": {
    "exec": "python ~/.config/wayside/gcal_fetch.py --quiet",
    "interval": 300,
    "allow-breaks": true,
    "on-click": "nyxt https://calendar.google.com/calendar/u/0/r"
  }
}
